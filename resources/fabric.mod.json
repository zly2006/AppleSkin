  "authors": [
    "squeek502"
  ],
  "contact": {
    "sources": "https://github.com/squeek502/AppleSkin",
    "homepage": "https://minecraft.curseforge.com/projects/appleskin",
    "issues": "https://github.com/squeek502/AppleSkin/issues"
  },

  "license": "Unlicense",
  "icon": "assets/appleskin/appleskin.png",

